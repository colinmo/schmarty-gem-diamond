{
    "repositories": [
        {
            "type": "path",
            "url": "../micropubkit",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "league/route": "^5.1",
        "league/plates": "^3.4",
        "league/container": "^4.2",
        "laminas/laminas-httphandlerrunner": "^2.1",
        "indieauth/client": "^1.1",
        "guzzlehttp/guzzle": "^7.0",
        "schmarty/micropubkit": "@dev",
        "masterminds/html5": "^2.8",
        "mf2/mf2": "^0.5.0",
        "indieweb/representative-h-card": "^0.1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "lib/config.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^10"
    }
}